{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "lines-between-class-members": "off",
    "no-underscore-dangle": "off",
    "import/extensions": "off"
  }
}